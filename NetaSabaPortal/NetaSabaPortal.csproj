				<None Remove="Resources\Images\icon_discord_25_black.png" />
				<None Remove="Resources\Images\icon_discord_25_gray.png" />
				<None Remove="Resources\Images\purplecat.png" />
				<None Remove="Resources\Images\roundCancel.png" />
				<None Remove="Resources\Images\roundChecked.png" />
		</ItemGroup>

		<ItemGroup>
				<Content Include="Resources\Images\purplecat_crophead.ico" />
		</ItemGroup>

		<ItemGroup>
				<PackageReference Include="CommunityToolkit.Mvvm" Version="8.2.2" />
				<PackageReference Include="Dapper" Version="2.1.24" />
				<PackageReference Include="DapperAid" Version="1.1.0" />
