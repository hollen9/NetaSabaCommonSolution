		<ItemGroup>
				<ProjectReference Include="..\Nogic.WritableOptions\Nogic.WritableOptions.csproj" />
				<ProjectReference Include="..\QueryMaster\QueryMaster.csproj" />
				<ProjectReference Include="..\SteamQueryNet\SteamQueryNet.csproj" />
				<ProjectReference Include="..\ValvePak\ValvePak.csproj" />
		</ItemGroup>

		<ItemGroup>
				<Resource Include="Resources\Images\animated\miku_sleep\miku_sleep.sprite.gif" />
				<Resource Include="Resources\Images\animated\miku_sleep\miku_sleep_awake_loop.gif" />
				<Resource Include="Resources\Images\animated\miku_sleep\miku_sleep_loop01.gif" />
				<Resource Include="Resources\Images\animated\miku_sleep\miku_sleep_wake.gif" />
				<Resource Include="Resources\Images\animated\padoru\padoru-smooth.gif" />
				<Resource Include="Resources\Images\icon_discord_25_black.png">
				  <CopyToOutputDirectory>Never</CopyToOutputDirectory>
				</Resource>
				<Resource Include="Resources\Images\icon_discord_25_gray.png">
				  <CopyToOutputDirectory>Never</CopyToOutputDirectory>
				</Resource>
				<Resource Include="Resources\Images\purplecat.png" />
				<Resource Include="Resources\Images\roundCancel.png">
						<CopyToOutputDirectory>Never</CopyToOutputDirectory>
				</Resource>
				<Resource Include="Resources\Images\roundChecked.png">
						<CopyToOutputDirectory>Never</CopyToOutputDirectory>
				</Resource>

		</ItemGroup>

		<!-- 新版 Visual Studio 會把資料夾底下的檔案都加進來，所以要先除掉一次，否則會編譯不過說有重複引用。-->
		<ItemGroup>
				<EmbeddedResource Remove="Resources\Locales\Strings.zh-tw.resx" />
				<EmbeddedResource Remove="Resources\Locales\Strings.zh-cn.resx" />
				<EmbeddedResource Remove="Resources\Locales\Strings.ja-jp.resx" />
				<EmbeddedResource Remove="Resources\Locales\Strings.ko-kr.resx" />
				<EmbeddedResource Remove="Resources\Locales\Strings.en-us.resx" />
		</ItemGroup>

		<ItemGroup>
				<EmbeddedResource Include="Resources\Locales\Strings.en-us.resx">
						<SubType>Designer</SubType>
						<DependentUpon>Strings.resx</DependentUpon>
				</EmbeddedResource>
				<EmbeddedResource Include="Resources\Locales\Strings.zh-cn.resx">
						<SubType>Designer</SubType>
						<DependentUpon>Strings.resx</DependentUpon>
				</EmbeddedResource>
				<EmbeddedResource Include="Resources\Locales\Strings.zh-tw.resx">
						<SubType>Designer</SubType>
						<DependentUpon>Strings.resx</DependentUpon>
				</EmbeddedResource>
				<EmbeddedResource Include="Resources\Locales\Strings.ko-kr.resx">
						<SubType>Designer</SubType>
						<DependentUpon>Strings.resx</DependentUpon>
				</EmbeddedResource>
				<EmbeddedResource Include="Resources\Locales\Strings.ja-jp.resx">
						<SubType>Designer</SubType>
						<DependentUpon>Strings.resx</DependentUpon>
				</EmbeddedResource>
		</ItemGroup>

		<ItemGroup>
		  <Resource Include="Resources\Images\discord_icon_clyde_black_RGB.svg">
		    <CopyToOutputDirectory>Never</CopyToOutputDirectory>
		  </Resource>
		</ItemGroup>

		<ItemGroup>
		  <Compile Update="Resources\Locales\Strings.Designer.cs">
		    <DesignTime>True</DesignTime>
		    <AutoGen>True</AutoGen>
		    <DependentUpon>Strings.resx</DependentUpon>
		  </Compile>
		</ItemGroup>

		<ItemGroup>
		  <EmbeddedResource Update="Resources\Locales\Strings.resx">
		    <Generator>ResXFileCodeGenerator</Generator>
		    <LastGenOutput>Strings.Designer.cs</LastGenOutput>
		  </EmbeddedResource>
		</ItemGroup>

		

		<ItemGroup>
				<None Update="config_advanced.jsonc">
				  <CopyToOutputDirectory>Always</CopyToOutputDirectory>
				</None>
				<None Update="config_entities.json">
						<CopyToOutputDirectory>Always</CopyToOutputDirectory>
				</None>
				<None Update="config.json">
						<CopyToOutputDirectory>Always</CopyToOutputDirectory>
				</None>
				<None Update="config_watcher.json">
				  <CopyToOutputDirectory>Always</CopyToOutputDirectory>
				</None>
		</ItemGroup>

</Project>
